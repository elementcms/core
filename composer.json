{
    "name": "elementcms/core",
    "description": "Easily manage, create and rollback your migrations in a user friendly dashboard",
    "keywords": ["laravel", "database", "migrations"],
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Day",
            "email": "peterday.main@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0"
    },
    "autoload": {
        "psr-4": {
            "Element\\Core\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "3.2-dev"
        },
        "laravel": {
            "providers": [
                "Element\\Core\\ServiceProvider"
            ]
        }
    },
    "require-dev": {
        "laravel/framework": "6.x.x",
        "genericmilk/cooker": "^2.2.0",
        "genericmilk/telephone": "^1.5.2",
        "genericmilk/rollout": "^1.0.0"
    }
}
